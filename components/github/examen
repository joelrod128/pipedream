#include <iostream>
using namespace std;
int main() {
  int a;
  int b;
  int c;
  int d;

  cout<<"CuÃ¡l es el rango inicial ?: ";
  cin>>a;
  cout<<"cual es el rango final?: ";
  cin>>b;
  cout<<"cual es el inicio de la tabla: ";
  cin>>c;
  cout<<"cual es el final de la tabla: ";
  cin>>d;

 for(int i=a; i<=b; i++){
        for(int j=c; j<=d; j++){
            
            
            cout <<a<<" * "<<j<<" = "<<a * j<<endl;
        }
        a++;
    }
}
